@model ICollection<TrainingManagement.Models.Student>



<hr />
<div class="container">
    
        <table class="table table-striped table-hover table-borderless">

            <thead>
                <tr>
                    <th>Name</th>
                    <th>Reg NO</th>
                    <th>Department</th>
                    <th>Email</th>
                    <th>Phone NO</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var student in @Model)
                {
                    <tr>
                        <td>@student.Name</td>
                        <td>@student.RegNo</td>
                        <td>@student.Department.Name</td>
                        <td>@student.Email</td>
                        <td>@student.PhoneNo</td>
                    </tr>
                }
            </tbody>
        </table>


    </div>



